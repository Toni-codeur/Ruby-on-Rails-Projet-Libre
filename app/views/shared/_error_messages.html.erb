<% if @utilisateur.errors.any? %>
  <div class="alert alert-danger">
    Le formulaire contient <%= pluralize(@utilisateur.errors.count, "erreur") %>.
  </div>
  <ul class="list-form">
  <% @utilisateur.errors.full_messages.each do |message| %>
    <li><%= message %></li>
  <% end %>
  </ul>
<% end %>